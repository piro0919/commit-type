{
  "activationEvents": [
    "onCommand:commit-type.addCommitType"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "commit-type.addCommitType",
        "icon": {
          "dark": "./static/images/icon-dark.png",
          "light": "./static/images/icon-light.jpg"
        },
        "title": "Add Commit Type"
      }
    ],
    "configuration": {
      "properties": {
        "commitType.displayDescription": {
          "default": true,
          "description": "Show description in quick pick",
          "type": "boolean"
        },
        "commitType.displayEmoji": {
          "default": false,
          "description": "Show emoji in commit type",
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "menus": {
      "scm/title": [
        {
          "command": "commit-type.addCommitType",
          "group": "navigation",
          "when": "scmProvider == git"
        }
      ]
    }
  },
  "description": "",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "displayName": "Commit Type",
  "engines": {
    "vscode": "^1.59.0"
  },
  "icon": "static/images/logo.png",
  "keywords": [
    "git",
    "commit",
    "message",
    "commitlint",
    "config-conventional",
    "config-angular"
  ],
  "main": "./out/extension.js",
  "name": "commit-type",
  "publisher": "kk-web",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}
